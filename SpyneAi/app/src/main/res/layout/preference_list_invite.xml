<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <View
        android:id="@+id/ellipse_713"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="@dimen/dp_15"
        android:background="@drawable/ellipse_713"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/tvName"
        android:layout_width="@dimen/dp_100"
        android:layout_height="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:fontFamily="@font/poppins_regular"
        android:gravity="center_vertical"
        android:text="Puru Malhotra"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_713"
        app:layout_constraintStart_toEndOf="@+id/ellipse_713"
        app:layout_constraintTop_toTopOf="@+id/ellipse_713"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>