<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/EnterpriseStartFlow">


    <fragment
        android:id="@+id/EnterpriseStartFlow"
        android:label="EnterpriseStartFlow"
        android:name="com.spyneai.registration.view.ui.fragment.signin.EnterpriseStartFlow"
        tools:layout="@layout/enterprise_start_flow">

        <action
            android:id="@+id/action_EnterpriseStartFlow_to_nav_phone_number_fragment"
            app:destination="@id/nav_phone_number_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
        <action
            android:id="@+id/action_EnterpriseStartFlow_to_JoinWorkspace2"
            app:destination="@id/JoinWorkspace"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
        <action
            android:id="@+id/action_EnterpriseStartFlow_to_nav_login_fragment"
            app:destination="@id/nav_login_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
        <action
            android:id="@+id/action_EnterpriseStartFlow_to_ChooseCategory"
            app:destination="@id/ChooseCategory"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
        <action
            android:id="@+id/action_EnterpriseStartFlow_to_nav_phone_number_fragment2"
            app:destination="@id/nav_phone_number_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>

    <fragment
        android:id="@+id/ChooseCategory"
        android:label="ChooseCategory"
        android:name="com.spyneai.onboardingv2.ui.intro.ChooseCategoryFragmentV2"
        tools:layout="@layout/fragment_choose_category2">

        <action
            android:id="@+id/action_ChooseCategory_to_nav_phone_number_fragment"
            app:destination="@id/nav_phone_number_fragment" />
        <action
            android:id="@+id/action_ChooseCategory_to_WebViewFragment"
            app:destination="@id/WebViewFragment" />
    </fragment>

    <fragment
        android:id="@+id/WebViewFragment"
        android:label="WebViewFragment"
        android:name="com.spyneai.onboardingv2.ui.intro.WebViewFragment"
        tools:layout="@layout/fragment_web_view">

        <action
            android:id="@+id/action_WebViewFragment_to_nav_login_fragment"
            app:destination="@id/nav_login_fragment" />

    </fragment>


    <fragment
        android:id="@+id/JoinWorkspace"
        android:label="JoinWorkspace"
        android:name="com.spyneai.registration.view.ui.fragment.EnterpriseWorkspacee"
        tools:layout="@layout/before_login">
        <action
            android:id="@+id/action_JoinWorkspace_to_nav_phone_otp_verify_fragment"
            app:destination="@id/nav_phone_otp_verify_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>

    <fragment
        android:id="@+id/nav_phone_number_fragment"
        android:label="PhoneNumber"
        android:name="com.spyneai.registration.view.ui.fragment.signup.RegistrationFragment"
        tools:layout="@layout/fragment_mobile_otp">
        <action
            android:id="@+id/action_fragment_mobile_otp_to_verify_otp"
            app:destination="@+id/nav_phone_otp_verify_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_fragment_mobile_otp_to_choose_your_plan_fragment"
            app:destination="@+id/nav_choose_plan_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_fragment_mobile_otp_to_email_with_otp"
            app:destination="@+id/nav_email_with_otp"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_fragment_sign_up_fragment_to_login_fragment"
            app:destination="@+id/nav_login_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popUpTo="@+id/mobile_navigation"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/nav_phone_otp_verify_fragment"
        android:name="com.spyneai.registration.view.ui.fragment.signup.OtpVerificationFragment"
        android:label="PhoneNumber"
        tools:layout="@layout/fragment_mobile_otp_varification">
        <action
            android:id="@+id/action_fragment_phone_otp_verify_fragment_user_info"
            app:destination="@+id/nav_user_info_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_fragment_phone_otp_verify_selected_fragment"
            app:destination="@+id/nav_selected_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_email_with_otp_to_main_activity"
            app:destination="@+id/nav_main_dashboard_activity"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_email_with_otp_to_main_v2_activity"
            app:destination="@+id/nav_main_v2_activity"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />


    </fragment>

    <fragment
        android:id="@+id/nav_email_with_otp"
        android:name="com.spyneai.registration.view.ui.fragment.signin.EmailWithOtpFragment"
        android:label="PhoneNumber"
        tools:layout="@layout/fragment_email_with_otp">
        <action
            android:id="@+id/action_fragment_email_otp_to_verify_otp"
            app:destination="@+id/nav_phone_otp_verify_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

    </fragment>

    <fragment
        android:id="@+id/nav_user_info_fragment"
        android:name="com.spyneai.registration.view.ui.fragment.signup.UserDetailFragment"
        android:label="User Details"
        tools:layout="@layout/fragment_user_detail">
        <action
            android:id="@+id/action_fragment_user_info_main_dashboard_activity"
            app:destination="@+id/nav_main_dashboard_activity"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

    </fragment>

    <fragment
        android:id="@+id/nav_selected_fragment"
        android:name="com.spyneai.registration.view.ui.fragment.signup.SelectedFragment"
        android:label="Selected Fragment"
        tools:layout="@layout/fragment_selected">
        <action
            android:id="@+id/action_fragment_selected_to_user_info_fragment"
            app:destination="@+id/nav_user_info_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_fragment_selected_to_validate_otp_"
            app:destination="@+id/nav_phone_otp_verify_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

    </fragment>



    <fragment
        android:id="@+id/nav_forget_pwd_fragment"
        android:name="com.spyneai.registration.view.ui.fragment.forgetpassword.ForgetPasswordFragment"
        android:label="ForgetPassword Fragment"
        tools:layout="@layout/fragment_forget_password">
        <action
            android:id="@+id/action_forget_pwd_fragment_to_login_fragment"
            app:destination="@+id/nav_login_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_forget_pwd_fragment_to_reset_fragment"
            app:destination="@+id/nav_reset_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

    </fragment>

    <fragment
        android:id="@+id/nav_login_fragment"
        android:name="com.spyneai.registration.view.ui.fragment.signin.LoginFragment"
        android:label="ForgetPassword Fragment"
        tools:layout="@layout/login_fragment">

        <action
            android:id="@+id/action_fragment_login_with_password_to_main_dashboard_activity"
            app:destination="@+id/nav_main_dashboard_activity"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_fragment_login_with_password_to_main_V2_activity"
            app:destination="@+id/nav_main_v2_activity"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_fragment_login_mobile_otp_to_verify_otp"
            app:destination="@+id/nav_phone_otp_verify_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />

        <action
            android:id="@+id/action_fragment_login_forget_pwd"
            app:destination="@+id/nav_forget_pwd_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />


        <action
            android:id="@+id/action_fragment_login_phone_number_fragment"
            app:destination="@+id/nav_phone_number_fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_nav_login_fragment_to_ChooseCategory"
            app:destination="@id/ChooseCategory" />


    </fragment>

    <fragment
        android:id="@+id/nav_reset_fragment"
        android:name="com.spyneai.registration.view.ui.fragment.forgetpassword.ResetPasswordFragment"
        android:label="ForgetPassword Fragment"
        tools:layout="@layout/fragment_reset_blank">


    </fragment>

    <activity
        android:id="@+id/nav_main_dashboard_activity"
        android:name="com.spyneai.dashboard.ui.MainDashboardActivity"
        android:label="Main Dashboard"
        tools:layout="@layout/activity_dashboard_main">

    </activity>

    <activity
        android:id="@+id/nav_main_v2_activity"
        android:name="com.spyneai.dashboardV2.ui.activity.MainV2Activity"
        android:label="Main V2"
        tools:layout="@layout/activity_main_v2">

    </activity>


</navigation>